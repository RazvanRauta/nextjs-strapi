{
  "kind": "collectionType",
  "collectionName": "news_posts",
  "info": {
    "singularName": "news-post",
    "pluralName": "news-posts",
    "displayName": "news-post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "date": {
      "type": "date",
      "default": "2021-11-30T21:00:00.000Z",
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true,
      "plugin": "upload"
    },
    "text": {
      "type": "richtext",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    }
  }
}