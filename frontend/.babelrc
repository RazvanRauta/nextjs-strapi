{
  "presets": [
    "next/babel"
  ],
  "env": {
    "production": {
      "plugins": [
        "emotion"
      ]
    },
    "development": {
      "plugins": [
        [
          "emotion",
          {
            "sourceMap": true,
            "autoLabel": "true"
          }
        ],
        ["module-resolver", {
          "root": ["./components"],
          "alias": {
            "pages": "./pages"
          }
        }]
      ]
    }
  }
}