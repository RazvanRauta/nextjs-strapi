{
  "presets": [
    "next/babel"
  ],
  "env": {
    "production": {
      "plugins": [
          "emotion"
      ],
      "presets": [
        "next/babel"
      ]
    },
    "development": {
      "plugins": [
        [
          "emotion",
          {
            "sourceMap": true,
            "autoLabel": true,
            "labelFormat": "[filename]--[local]",
            "cssPropOptimization": true
          }
        ]
      ],
      "presets": [
        "next/babel"
      ]
    }
  },
  "plugins": [
    [
      "emotion"
    ]
  ]
}